{
  "name": "stephen-oneil-resume",
  "version": "3.0.0",
  "homepage": "https://resume.oneil.online",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "spellcheck": "node scripts/spellcheck_content.js",
    "build": "astro build && node scripts/generate_pdf.js",
    "test-pdf-length": "node scripts/test_pdf_page_count.js",
    "prelocal-prod": "npm run build && npm run test-pdf-length",
    "local-prod": "http-server dist -c-1 -a 127.0.0.1 -p 3000 -s"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@astrojs/ts-plugin": "1.10.6",
    "@eslint/js": "9.39.1",
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/lodash": "4.17.21",
    "@types/node": "24.10.1",
    "astro": "5.16.4",
    "classnames": "2.5.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "get-port": "7.1.0",
    "globals": "16.5.0",
    "http-server": "14.1.1",
    "jiti": "2.6.1",
    "lodash": "4.17.21",
    "marked": "17.0.1",
    "moment": "2.30.1",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "puppeteer": "24.32.0",
    "remove-markdown": "0.6.2",
    "sass-embedded": "1.93.3",
    "serve-handler": "6.1.6",
    "simple-spellchecker": "0.9.9",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1"
  }
